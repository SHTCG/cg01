@import "gnb.css";
@import "class.css";

/* |  tag & id  | */
* {margin:0;padding:0;}
body {text-align:center;}
#wrap {width:940px;margin:0 auto;padding-right:55px;font:12px 'arial','dotum';text-align:left;}
#wrap:after {display:block;overflow:hidden;clear:both;content:''}
#iframeWrap {text-align:left;}
#w940 {width:940px;padding:0;}
#w749 {width:749px;padding:0;}
ul {list-style:none;}
input {vertical-align:middle;}
h1, h2 {clear:both;float:left;}
frame, iframe { border:0; }
address,caption,cite,code,dfn,em,i,var{font-style:normal}

/* |  font  | */
body, td, div, h1, h2, h3, h4, select, input, textarea {font:normal 12px 'arial','dotum','arial';color:#8b8d8e;}

/* |  img  | */
img {border:0;}
.png24 {tmp:expression(setPng24(this));}
li img {vertical-align:top;}
dt img ,dd img {vertical-align:top;}

/* |  a  | */
a   {color:#8b8d8e;text-decoration:none;}
a:hover {color:#8b8d8e;text-decoration:underline;}

/* |  layout  | */
#layBody {clear:both;width:940px;padding-top:15px;overflow:hidden}
#layBody:before {display:block;overflow:hidden;clear:both;content:''}
#layLeft {float:left;width:176px;padding-right:15px;}
#layRight {float:left;width:749px;}

/* | cate & location bar | */
#catePosition {clear:both;float:left;width:940px;margin-top:12px;}
#cateAllView {position:relative;float:left;width:176px;height:21px;margin-right:15px;z-index:11990;}
	#gnbNaviArea {position:absolute;top:0;left:0;width:176px;height:21px;overflow:hidden;}
#pageLocationArea {position:relative;float:left;width:749px;color:#000;text-align:left;z-index:2600;}
	#pageLocationArea .home {padding:4px 20px 0 0;background:url('http://8986825504616229465-a-1802744773732722657-s-sites.googlegroups.com/site/picthst17817/gtes/bg_cp_div.gif') no-repeat 100% 0;color:#5C5C5C;}
		#pageLocationArea .home a {color:#5C5C5C;}
	#pageLocationArea .noBg {padding-right:0;background:transparent;}
	#pageLocationArea div	{position:relative;float:left;}
	#pageLocationArea .div {padding-right:20px;background:url('http://8986825504616229465-a-1802744773732722657-s-sites.googlegroups.com/site/picthst17817/gtes/bg_cp_div.gif') no-repeat 100% 0;}
	#pageLocationArea .selTxt {width:147px;height:15px;padding:4px 0 0 14px;background:url('http://8986825504616229465-a-1802744773732722657-s-sites.googlegroups.com/site/picthst17817/gtes/bg_cp_arrow.gif') no-repeat 100%;color:#5C5C5C;border:1px solid #D2D2D2;cursor:pointer;}
	#pageLocationArea span {display:block;}

