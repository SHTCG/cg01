/* | footer | */
#footer {clear:both;float:left;position:relative;width:100%;height:88px;margin:30px auto 0;background:url('http://8986825504616229465-a-1802744773732722657-s-sites.googlegroups.com/site/picthst17817/gtes/bg_footer_top.gif') repeat-x;text-align:center;z-index:1;}
	#footer div {width:995px;margin:0 auto;padding-top:27px;text-align:left;}

/* | quick view | */
#preOrderView {position:absolute;}

/* | calendar | */
#calendar {position:relative;width:181px;height:150px;padding:40px 10px 10px;background:#BED600;}
	#calendar select {color:#000;}
	#calendar .year {position:absolute;top:10px;left:10px;}
	#calendar .month {position:absolute;top:10px;right:10px;}
	#calendar .day {width:181px;height:118px;padding-top:12px;background:url('http://8986825504616229465-a-1802744773732722657-s-sites.googlegroups.com/site/picthst17817/gtes/bg_calendar.gif') no-repeat;}
		#calendar td {height:22px;padding-right:5px;color:#8B8D8E;text-align:right;}
		#calendar td span {font-weight:bold;color:#2C2A1A;}
	#calendar .close {position:absolute;bottom:10px;right:10px;font-size:11px;color:#5A5B5C;cursor:pointer;}

.error {clear:both;width:100%;margin:55px 0 45px;text-align:center;}
	.error p {border-bottom:1px solid #E1E4E6;}
	.error .txt {padding:20px 0;font:bold 14px 'dotum';color:#2c2a1a;}
	.error .txt span {font:bold 15px 'arial';color:#2c2a1a;}